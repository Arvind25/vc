#av-menu-outer
		#av-indicator.flash(style="display:none;")
		div(style="display: inline-block;")
			#av-menu-audio-unmute.av-menu-item.text-danger(style="display: none;")
				include images/microphone58-slashed.svg
			#av-menu-audio-mute.av-menu-item.disabled
				include images/microphone58.svg
		div(style="display: inline-block;")
			#av-menu-video-unmute.av-menu-item.text-danger(style="display: none;")
				include images/camera-slash.svg
			#av-menu-video-mute.av-menu-item.disabled
				include images/camera.svg
